		"@biesbjerg/ngx-translate-extract": "2.3.4",
		"@types/cheerio": "0.22.7",
		"@types/del": "3.0.1",
		"@types/dotenv": "4.0.3",
		"@types/fs-extra": "5.0.1",
		"@types/inflection": "1.5.28",
		"@types/lodash": "4.14.106",
		"@types/loglevel": "1.5.3",
		"cheerio": "1.0.0-rc.2",
