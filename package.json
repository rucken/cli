{
	"name": "@rucken/cli",
	"version": "2.0.6",
	"description": "Console tools for create and build Angular5+ application based on rucken",
	"bugs": {
		"url": "https://github.com/rucken/cli/issues"
	},
	"homepage": "https://github.com/rucken/cli",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rucken/cli.git"
	},
	"license": "MIT",
	"scripts": {
		"clean": "del-cli dist",
		"build": "npm run clean && tsc --pretty",
		"test": "npm run build && mocha --timeout 3000000 --require ts-node/register --recursive test/**/*._spec.ts",
		"watch": "npm run build -- --watch",
		"watch:test": "npm run test -- --watch"
	},
	"keywords": [
		"angular5",
		"web application",
		"seed",
		"i18n",
		"scaffold",
		"rucken"
	],
	"author": {
		"name": "endy",
		"email": "admin@site15.ru"
	},
	"engines": {
		"node": ">=6",
		"npm": ">=5.3.0"
	},
	"main": "dist/bin/app.js",
	"typings": "dist/bin/app.d.ts",
	"bin": {
		"rucken": "dist/bin/app.js"
	},
	"files": [
		"bin",
		"dist",
		"scripts",
		"srcgen"
	],
	"devDependencies": {
		"@types/chai": "4.1.2",
		"@types/mocha": "5.0.0",
		"@types/node": "9.6.4",
