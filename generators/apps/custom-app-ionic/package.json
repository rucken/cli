  "scripts": {
    "config:dev": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:dev": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build": "run-s prepare:prod lib:build-ionic",
    "app:prebuild": "ng build app --prod -c=production",
    "app:prebuild:dev": "ng build app --prod -c=dev",
    "app:build": "run-s prepare:dev app:prebuild",
    "app:build:dev": "run-s prepare:dev app:prebuild:dev",
    "ng": "ng",
    "serve": "ng serve -c=production --port=8000",
    "serve:dev": "ng serve -c=dev --port=8000",
    "start:prod": "run-s prepare:dev serve",
    "start:dev": "run-s prepare:dev serve:dev",
    "build": "run-s clean config:dev app:build",
    "build:dev": "run-s clean config:dev app:build:dev",
    "test": "run-s build",
    "lint": "ng lint",
    "clean": "./node_modules/.bin/del-cli dist",
    "e2e": "ng e2e",
    "android": "ng run app:android-build",
    "postinstall": "sh ./scripts/postinstall.sh",
    "github:prebuild": "ng build app --base-href /custom-app-ionic-ionic/ --prod",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/custom-app-ionic/index.html ./dist/custom-app-ionic/404.html",
    "github:build": "run-s lib:build config:prod github:prebuild github:postbuild doc",
    "github:deploy": "gh-pages -d dist/custom-app-ionic",
    "doc:app:custom-app-ionic": "./node_modules/.bin/compodoc -p apps/custom-app-ionic/tsconfig.app.json -n \"rucken:ionic-custom-app-ionic\" -d dist/custom-app-ionic/doc/custom-app-ionic/",
    "doc": "run-s doc:app:custom-app-ionic",
    "all:build": "run-s github:build android"
  },
